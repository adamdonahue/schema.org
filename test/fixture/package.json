{
  "name": "mydpkg",
  "version": "0.0.0",
  "description": "a description",
  "keywords": ["hyperdata", "json-ld", "test"],
  "dataDependencies": ["dpkg1/0.0.1", "dpkg2/0.2.1", "http://data.com/"],
  "author": {
    "name": "Sebastien Ballesteros",
    "email": "sebastien@standardanalytics.io"
  },

  "code": [
    {
      "codeRepository": "git://github.com/standard-analytics/data-analysis.git",
      "programmingLanguage": {"name": "R"},
      "runtime": "R"
    }
  ],

  "dataset": [
    {
      "name": "myurl",
      "url": "http://data.com",
      "distribution": {
        "contentUrl" :"/mydpkg/0.0.0/resource0/myurl",
        "contentSize" : 1024,
        "encodingFormat": "txt",
        "hashAlgorithm": "md5",
        "hashValue": "d131dd02c5e6eec4"        
      }
    },
    {
      "name": "mycsv",
      "path": "data/data.csv",
      "distribution": {
        "contentUrl" :"/mydpkg/0.0.0/resource0/mycsv",
        "contentSize" : 1024,
        "encodingFormat": "csv",
        "hashAlgorithm": "md5",
        "hashValue": "d131dd02c5e6eec4"
      }
    },
    {
      "name": "mydata",
      "data": { "inline": "data" },
      "distribution": {
        "contentUrl" :"/mydpkg/0.0.0/resource0/mydata",
        "contentSize" : 1024,
        "encodingFormat": "json",
        "hashAlgorithm": "md5",
        "hashValue": "d131dd02c5e6eec4"
      }
    }
  ]
}
