{
  "name": "mypkg",
  "version": "0.0.0",
  "description": "a description",
  "keywords": ["hyperdata", "json-ld", "test"],
  "isBasedOnUrl": ["pkg1/0.0.1", "pkg2/0.2.1", "http://data.com/"],
  "author": {
    "name": "Sebastien Ballesteros",
    "email": "sebastien@standardanalytics.io"
  },

  "dataset": [
    {
      "name": "myurl",
      "distribution": [ { "contentUrl" :"http://data.com" } ]
    },
    {
      "name": "mycsv",
      "distribution": [ { "contentPath" :"data/data.csv" } ]
    },
    {
      "name": "mydata",
      "isBasedOnUrl": [ "mypkg/0.0.0/sourceCode/myanalytics" ],
      "distribution": [ { "contentData": { "inline": "data" } } ]
    }
  ],

  "sourceCode": [
    {
      "name": "myanalytics",
      "targetProduct": [{
        "operatingSystem": "Unix",
        "input": [ "mypkg/0.0.0/dataset/mycsv" ]
      }]
    }
  ],

  "image": [
    {
      "name": "myfig",
      "encoding": [ { "contentPath": "image/x.png" } ],
      "isBasedOnUrl": [ "mypkg/0.0.0/dataset/mydata",  "mypkg/0.0.0/sourceCode/myanalytics" ]
    }
  ],

  "article": [
    {
      "name": "myarticle",
      "encoding": [ { "contentPath": "article/article.pdf" } ]
    }
  ]
}
